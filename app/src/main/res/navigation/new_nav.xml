<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/new_nav">

    <fragment
        android:id="@+id/homie"
        android:name="com.example.chashma99com.fragment.HomieFragment"
        android:label="fragment_homie"
        tools:layout="@layout/fragment_homie" >
        <action
            android:id="@+id/action_homie_to_categoriesFragment"
            app:destination="@id/categoriesFragment" />
        <action
            android:id="@+id/action_homie_to_cartFragment"
            app:destination="@id/cartFragment" />
        <action
            android:id="@+id/action_homie_to_productFragment"
            app:destination="@id/productFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.example.chashma99com.categories.CategoriesFragment"
        android:label="fragment_categories"
        tools:layout="@layout/fragment_categories" >
        <action
            android:id="@+id/action_categoriesFragment_to_wishlistrFragment"
            app:destination="@id/wishlistrFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_computerFragment"
            app:destination="@id/computerFragment" />
    </fragment>
    <fragment
        android:id="@+id/wishlistrFragment"
        android:name="com.example.chashma99com.fragment.WishlistrFragment"
        android:label="fragment_wishlistr"
        tools:layout="@layout/fragment_wishlistr" >
        <action
            android:id="@+id/action_wishlistrFragment_to_profile2"
            app:destination="@id/profile2" />
    </fragment>
    <fragment
        android:id="@+id/profile2"
        android:name="com.example.chashma99com.fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.chashma99com.fragment.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/productFragment"
        android:name="com.example.chashma99com.fragment.ProductFragment"
        android:label="fragment_product"
        tools:layout="@layout/fragment_product" />
    <fragment
        android:id="@+id/computerFragment"
        android:name="com.example.chashma99com.categories.ComputerFragment"
        android:label="fragment_computer"
        tools:layout="@layout/fragment_computer" />
</navigation>